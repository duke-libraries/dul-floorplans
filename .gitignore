# Ignore bundler config.
/.bundle

# Ignore bin directory to hopefully 
# make it easier to manage project cross-platform
/bin

# Ignore the media directory
/app/assets/media/*

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp
