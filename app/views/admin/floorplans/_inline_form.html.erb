<div class="modal fade" role="dialog" id="diagInlineEditFloorArea" aria-hidden="true">
	<div class="modal-dialog modal-lg">
		<div class="modal-content">
      <div class="modal-body">
        <form class="form">
				  <div class="form-group">
				    <label for="editFloorAreaCoord">Coordinates</label>
				    <textarea class="form-control input-sm" rows="4" name="coord" id="editFloorAreaCoord"></textarea>
				  </div>
				  <div class="form-group">
				    <label for="editFloorAreaShape">Shape</label>
				    <%= select_tag(:shape, options_for_select(@area_shapes), {class: 'form-control input-sm', id: 'editFloorAreaShape' }) %>
				  </div>
				  <input type="hidden" class="paramAreaId" id="editFloorAreaId" />
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" id="primaryButton" data-floor-area-id="" class="btn btn-primary" data-url="">Save Changes</button>
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
      </div>
		</div>
	</div>
</div>
