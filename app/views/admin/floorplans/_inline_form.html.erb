<div class="modal fade" role="dialog" id="diagInlineEditArea" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
      <div class="modal-body">
        <form class="form">
				  <div class="form-group">
				    <label for="editFloorAreaCoord">Coordinates</label>
				    <input type="text" class="form-control input-sm" id="editFloorAreaCoord">
				  </div>
				  <div class="form-group">
				    <label for="editFloorAreaShape">Shape</label>
				    <%= select_tag(:area_shape, options_for_select(@area_shapes), {class: 'form-control input-sm', id: 'editFloorAreaShape' }) %>
				  </div>
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-primary">Save Changes</button>
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
      </div>
		</div>
	</div>
</div>
